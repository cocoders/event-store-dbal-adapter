{
    "name": "cocoders/event-store-dbal-adapter",
    "license": "MIT",
    "description": "DBAL adapter for cocoders/event-store",
    "autoload": {
        "psr-4": {"": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Tests\\": "tests"}
    },
    "require": {
        "php": "^7.0",
        "cocoders/event-store": "0.1.*@dev",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "5.2.*",
        "ramsey/uuid": "^3.1",
        "jms/serializer": "1.1.0",
        "symfony/serializer": "3.0.3"
    },
    "suggest": {
        "jms/serializer": "When you want to use jms serializer",
        "symfony/serialize": "When you want to use symfony serializer"
    },
    "config": {
        "bin-dir": "bin"
    }
}
